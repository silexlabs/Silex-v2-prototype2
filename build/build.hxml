#########
# builder
#########
-js ../bin/publications/admin/silex-builder.js
-cp ../src/
-cp ../../Brix/src/
-cp ../../Cocktail/src/
#--no-traces
-debug
-lib hscript

-main silex.Silex
--macro brix.core.Builder.create('../src/index-builder.html', '../bin/publications/admin/index.html')

#-D brixdebug
-D disableEmbedHtml
-D noAutoStart
-D stopOnError

-D silexClientSide
-D silexDebug
-D redirectFlashTraces

--next
#########
# player JS
#########
-js ../bin/libs/silex/silex.js
-cp ../src/
-cp ../../Brix/src/
-cp ../../Cocktail/src/
#--no-traces
-debug
-lib hscript

-main brix.core.Application
--macro brix.core.Builder.create('../src/index-player.html')

#-D brixdebug
-D disableEmbedHtml
-D noAutoStart
-D stopOnError

-D silexClientSide
-D silexDebug
-D redirectFlashTraces
